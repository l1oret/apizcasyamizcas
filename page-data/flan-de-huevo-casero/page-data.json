   srcset=\"/static/483d6923d150f2cb6347d43a46a61420/ff44c/IMG_3374-pizcas.jpg 158w,\n/static/483d6923d150f2cb6347d43a46a61420/a6688/IMG_3374-pizcas.jpg 315w,\n/static/483d6923d150f2cb6347d43a46a61420/828fb/IMG_3374-pizcas.jpg 630w,\n/static/483d6923d150f2cb6347d43a46a61420/c08c5/IMG_3374-pizcas.jpg 640w\"\n        sizes=\"(max-width: 630px) 100vw, 630px\"\n        style=\"width:100%;height:100%;margin:0;vertical-align:middle;position:absolute;top:0;left:0;\"\n        loading=\"lazy\"\n      />\n  </a>\n    </span></p>","frontmatter":{"title":"Flan de huevo casero","date":"December 20, 2012","description":null}},"previous":{"fields":{"slug":"/arroz-con-verduras/"},"frontmatter":{"title":"Arroz con verduras"}},"next":{"fields":{"slug":"/pasta-con-gambas/"},"frontmatter":{"title":"Pasta con gambas y calabacín"}}},"pageContext":{"id":"a875f9fa-1d0e-5d34-a280-efe7b9f06f92","previousPostId":"c2012d92-e00c-5c38-b059-2a1fc70e0dbc","nextPostId":"331cdf0b-799b-54b9-8931-a4b09e96a375"}},"staticQueryHashes":["2841359383","3257411868"]}